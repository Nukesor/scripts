rm -rf ~/.python_history
rm -rf ~/.putty

# Java stuff
rm -rf ~/.java
rm -rf ~/.gradle
rm -rf ~/.rye

# Wtf
rm -rf ~/.esd_auth
rm -rf ~/.sbd
rm -rf ~/GPUCache
rm -rf ~/.gnome
rm -rf ~/.android

# Desktop/File manager
rm -rf ~/.local/share/Trash/*
