#!/bin/bash
rm -rf ~/Desktop
rm -rf ~/Documents

# Cache stuff
rm -rf ~/.ansible
rm -rf ~/.pkg-cache
rm -rf ~/.cmake
rm -rf ~/.subversion
rm -rf ~/.dotnet
rm -rf ~/.ipfs
rm -rf ~/.nuget
rm -rf ~/.mono
rm -rf ~/.berkshelf

# History stuff
rm -rf ~/.wget-hsts
rm -rf ~/.NERDTreeBookmarks
